{
  "name": "nuxt-github-pages",
  "version": "1.5.0",
  "description": "Nuxt module to fix trailing slash issues for GitHub Pages deployments",
  "repository": "mitre/nuxt-github-pages",
  "license": "Apache-2.0",
  "keywords": [
    "nuxt",
    "github-pages",
    "static-site",
    "deployment",
    "trailing-slash"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "./scripts/release.sh",
    "lint": "eslint .",
    "test": "pnpm run dev:prepare && vitest run",
    "test:watch": "pnpm run dev:prepare && vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "test:clean": "pnpm run clean && pnpm test",
    "clean": "./scripts/clean.sh",
    "test:all": "./scripts/test-all.sh",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^4.0.2",
    "@nuxt/test-utils": "^3.19.1",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.27.0",
    "execa": "^9.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "nuxt": "^3.17.4",
    "typescript": "~5.9.2",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@9.14.4+sha512.c8180b3fbe4e4bca02c94234717896b5529740a6cbadf19fa78254270403ea2f27d4e1d46a08a0f56c89b63dc8ebfd3ee53326da720273794e6200fcf0d184ab"
}
